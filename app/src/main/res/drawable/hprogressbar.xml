<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <stroke android:width="1dp" android:color="@color/register_textbox"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#3fc64a" android:endColor="#3fc64a" android:angle="0" android:centerY="0.75" android:centerColor="#3fc64a"/>
            </shape>
        </clip>
    </item>
</layer-list>
