<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:my="http://schemas.android.com/apk/res/com.contec.phms" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:user="http://schemas.android.com/apk/res-auto" xmlns:app="http://schemas.android.com/apk/res/com.contec" android:padding="5dp" android:layout_width="match_parent" android:layout_height="match_parent">
    <ImageView android:id="@+id/iv_notification_contentview_icon" android:layout_width="wrap_content" android:layout_height="wrap_content" user:srcCompat="@drawable/img_phms_icon"/>
    <LinearLayout android:gravity="center_vertical" android:orientation="vertical" android:paddingLeft="8dp" android:paddingRight="5dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_toRightOf="@+id/iv_notification_contentview_icon" android:layout_alignBottom="@+id/iv_notification_contentview_icon">
        <TextView android:ellipsize="end" android:id="@+id/tv_notification_data" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxLines="2"/>
    </LinearLayout>
</RelativeLayout>
