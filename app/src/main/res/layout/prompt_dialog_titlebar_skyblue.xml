<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:my="http://schemas.android.com/apk/res/com.contec.phms" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:user="http://schemas.android.com/apk/res-auto" xmlns:app="http://schemas.android.com/apk/res/com.contec" android:orientation="vertical" android:id="@+id/dialog" android:background="@drawable/fynn_prompt_dialog_bg_normal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="30dp" android:layout_marginRight="30dp">
    <LinearLayout android:gravity="left|center_vertical|center_horizontal|center" android:id="@+id/titlebar" android:background="@drawable/fynn_prompt_dialog_title_bar_bg_skyblue" android:paddingLeft="15dp" android:paddingTop="10dp" android:paddingRight="15dp" android:paddingBottom="10dp" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="20sp" android:textColor="#ffffff" android:gravity="center" android:layout_gravity="center_vertical" android:id="@+id/title" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    </LinearLayout>
    <View android:id="@+id/title_msg_divider" android:background="#dcdcdc" android:visibility="visible" android:layout_width="match_parent" android:layout_height="1px" android:layout_marginBottom="20dp"/>
    <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/layout_addview" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="15dp" android:layout_marginRight="15dp" android:layout_marginBottom="20dp">
        <TextView android:textSize="18sp" android:textColor="#696969" android:gravity="left" android:id="@+id/message" android:visibility="visible" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="dsadas"/>
    </LinearLayout>
    <View android:id="@+id/title_msg_divider2" android:background="#dcdcdc" android:visibility="gone" android:layout_width="match_parent" android:layout_height="1px" android:layout_marginBottom="20dp"/>
    <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/layout_addview2" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="15dp" android:layout_marginRight="15dp" android:layout_marginBottom="20dp">
        <TextView android:textSize="18sp" android:textColor="#696969" android:gravity="left" android:id="@+id/message2" android:visibility="visible" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="dsadaddsads"/>
    </LinearLayout>
    <View android:id="@+id/msg_btn_divider" android:background="#dcdcdc" android:visibility="gone" android:layout_width="match_parent" android:layout_height="1px"/>
    <LinearLayout android:id="@+id/btn_view" android:layout_width="match_parent" android:layout_height="50dp">
        <TextView android:textSize="18sp" android:textColor="#808080" android:gravity="center" android:id="@+id/button_left" android:background="@drawable/fynn_prompt_dialog_btn_left_selector" android:visibility="gone" android:clickable="true" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1"/>
        <View android:id="@+id/btn_divider1" android:background="#dcdcdc" android:visibility="gone" android:layout_width="1px" android:layout_height="wrap_content"/>
        <TextView android:textSize="18sp" android:textColor="#808080" android:gravity="center" android:id="@+id/button_center" android:background="@drawable/fynn_prompt_dialog_btn_center_selector" android:visibility="gone" android:clickable="true" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1"/>
        <View android:id="@+id/btn_divider2" android:background="#dcdcdc" android:visibility="gone" android:layout_width="1px" android:layout_height="wrap_content"/>
        <TextView android:textSize="18sp" android:textColor="#808080" android:gravity="center" android:id="@+id/button_right" android:background="@drawable/fynn_prompt_dialog_btn_right_selector" android:visibility="gone" android:clickable="true" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1"/>
    </LinearLayout>
</LinearLayout>
